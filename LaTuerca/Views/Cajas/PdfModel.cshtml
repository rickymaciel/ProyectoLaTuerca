@model LaTuerca.Models.Caja
@{
    Layout = "~/Views/Shared/_PdfLayout.cshtml";
}
<paragraph style="font-family:Helvetica;font-size:18px;font-style:italic;">
	<chunk style="font-weight:bold;">@ViewBag.Title</chunk>
</paragraph>
<table width="100" cellpadding="0" cellspacing="0" widths="30;60" borderwidth="1.0" left="false" right="false" top="false" bottom="false" red="0" green="0" blue="0">
    <row>
        <cell>
            <chunk style="font-weight:bold;">UserName:</chunk>
        </cell>
        <cell>
            <chunk style="">@Model.Inicial</chunk>
        </cell>
    </row>
    <row>
        <cell>
            <chunk style="font-weight:bold;">Lucky Number:</chunk>
        </cell>
        <cell>
            <chunk style="">@Model.Cierre</chunk>
        </cell>
    </row>
</table>

